{
   "comments": {
       "lineComment": "//",
       "blockComment": [ "/*", "*/" ]
   },
   "brackets": [
       ["{", "}"],
       ["[", "]"],
       ["(", ")"]
   ],
   "autoClosingPairs": [
       { "open": "{", "close": "}" },
       { "open": "[", "close": "]" },
       { "open": "(", "close": ")" },
       { "open": "\"", "close": "\"", "notIn": ["string"] },
       { "open": "#\"", "close": "\"#", "notIn": ["string"] },
       { "open": "##\"", "close": "\"##", "notIn": ["string"] },
       { "open": "###\"", "close": "\"###", "notIn": ["string"] },
       { "open": "\"\"\"", "close": "\"\"\"", "notIn": ["string"] },
       { "open": "#\"\"\"", "close": "\"\"\"#", "notIn": ["string"] },
       { "open": "##\"\"\"", "close": "\"\"\"##", "notIn": ["string"] },
       { "open": "###\"\"\"", "close": "\"\"\"###", "notIn": ["string"] },
       { "open": "`", "close": "`", "notIn": ["string", "comment"] }
   ],
   "surroundingPairs": [
       ["{", "}"],
       ["[", "]"],
       ["(", ")"],
       ["\"", "\""],
       ["#\"", "\"#"],
       ["##\"", "\"##"],
       ["\"\"\"", "\"\"\""],
       ["#\"\"\"", "\"\"\"#"],
       ["##\"\"\"", "\"\"\"##"],
       ["`", "`"]
   ],
   "folding": {
       "offSide": false,
       "markers": {
           "start": "^\\s*//#region",
           "end": "^\\s*//#endregion"
       }
   }
}